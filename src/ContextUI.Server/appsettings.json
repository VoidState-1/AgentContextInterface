{
  "ContextUI": {
    "Render": {
      "MaxTokens": 8000,
      "MinConversationTokens": 2000
    },
    "Context": {
      "MaxItems": 100
    },
    "ActivityLog": {
      "MaxLogs": 50
    }
  },
  "OpenRouter": {
    "BaseUrl": "https://openrouter.ai/api/v1",
    "ApiKey": "",
    "DefaultModel": "anthropic/claude-3.5-sonnet",
    "FallbackModels": [
      "openai/gpt-4-turbo",
      "google/gemini-pro"
    ],
    "MaxTokens": 4096,
    "Temperature": 0.7,
    "TimeoutSeconds": 60,
    "MaxRetries": 3
  },
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning"
    }
  },
  "AllowedHosts": "*"
}
